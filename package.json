{
  "name": "base-mern-ts",
  "version": "1.0.0",
  "description": "A simple boilerplate for a full MERN stack, using Mongodb, Express.js, React.js and Node.js with Typescript",
  "main": "index.js",
  "scripts": {
    "clean": "sh clean.sh",
    "build": "sh build.sh",
    "deploy": "sh deploy.sh",
    "prep:server": "cd src/server && npm i",
    "prep:client": "cd src/client && npm i",
    "start:server": "cd src/server && npm start",
    "start:client": "cd src/client && npm start"
  },
  "keywords": [
    "typescript",
    "node",
    "express",
    "react",
    "mongodb",
    "docker",
    "docker-compose"
  ],
  "author": "Carlos Barbosa <socarlosn@gmail.com>",
  "license": "MIT"
}
